#include <iostream>
#include <vector>
using namespace std;

void TestParametres(vector<int>& parametres) {
    cout << "nombre de parametres est : " << parametres.size()<<endl;


}

int main() {
    try {
      for(size_t i =1;;i*=3){
        vector<int>parametres(i,1);
         TestParametres(parametres);

}} catch (const exception& e) {
      cerr << "Error  : " << e.what() << endl;
    }
return 0;

}
